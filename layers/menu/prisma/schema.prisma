generator client {
    provider = "prisma-client"
    output   = "../generated/prisma"
}

datasource db {
    provider = "postgresql"
}

model WeeklyMenu {
    id        String   @id @default(uuid())
    name      String
    type      String
    isActive  Boolean
    startDate DateTime
    endDate   DateTime
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

model Dish {
    id       String @id @default(uuid())
    name     String
    calories Int
}
